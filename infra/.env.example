# .env.example
# Copy this file to .env and replace with your actual secrets
# (Never commit your real .env to GitHub)

# Postgres database connection string (example format from Neon)
DATABASE_URL="postgresql://USER:PASSWORD@HOST/DBNAME?sslmode=require&channel_binding=require"

# Cloudflare account details (to be added in Phase 1/2)
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token
